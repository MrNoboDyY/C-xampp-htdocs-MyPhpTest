{# empty Twig template #}
<h1>Fruits</h1>
<table border="5">
    <thead>
    <tr>
        <th>Fruits</th> 
        <th>Color</th>
    </tr>
    </thead>
    <tbody>
        {% for fruit in fruits %}
    <tr>
        <td><font color="{{fruit.couleur}}">{{fruit.name}}</font></td>
        <td><font color="{{fruit.couleur}}">{{fruit.couleur}}</font></td>
    </tr>
        {%endfor%}
    </tbody>
</table>
